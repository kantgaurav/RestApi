package pojo;

import org.apache.juneau.json.JsonSerializer;

public class Serialization_pojo2JSON {
	
	//#juneau-marshall API
	
	public static void main(String[] args) {
		

	String [] sellerNames = {"Lenova", "Samsung", "Mac"};
   JsonSerializer js = JsonSerializer.DEFAULT_READABLE;
   
   Product p = new Product("M10Plus", 27664, "Grey", sellerNames);
   
  String json= js.serialize(p);
  
  System.out.println(json);
   
	}
   
   
	
	

}
